header {
  position: relative;
  // background: $black;
  height: 100vh;
  width: rem(60);
  
  h1 {
    font-family: Helvetica;
    font-size: 24px;
    font-weight: 700;
    padding-left: rem(20);
    position: absolute;
    left: 0;
    bottom: 0;
    line-height: rem(60);
    right: 0;
    transform: rotate(-90deg);
    white-space nowrap;
    // color: $white;
  }
}
